{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "react/prop-types": [0],
    "no-undef": 0,
    "import/no-dynamic-require": 0,
    "global-require": 0
  }
}